execute if score time cmzy_timer matches 20 run title @a title ["",{"text":"准备...","color":"yellow","bold":true}]
execute if score time cmzy_timer matches 21 run function cmzy:random
execute if score time cmzy_timer matches 60 run title @a title ["",{"text":"■■■■■","color":"green","bold":true}]
execute if score time cmzy_timer matches 80 run title @a title ["",{"text":"■■■■","color":"green","bold":true}]
execute if score time cmzy_timer matches 100 run title @a title ["",{"text":"■■■","color":"gold","bold":true}]
execute if score time cmzy_timer matches 120 run title @a title ["",{"text":"■■","color":"gold","bold":true}]
execute if score time cmzy_timer matches 140 run title @a title ["",{"text":"■","color":"red","bold":true}]
execute if score time cmzy_timer matches 60 run playsound block.note_block.bell master @a ~ ~ ~ 999999 1
execute if score time cmzy_timer matches 80 run playsound block.note_block.bell master @a ~ ~ ~ 999999 1
execute if score time cmzy_timer matches 100 run playsound block.note_block.bell master @a ~ ~ ~ 999999 1.5
execute if score time cmzy_timer matches 120 run playsound block.note_block.bell master @a ~ ~ ~ 999999 1.5
execute if score time cmzy_timer matches 140 run playsound block.note_block.bell master @a ~ ~ ~ 999999 2
execute if score time cmzy_timer matches 160 run title @a title ["",{"text":"晚安","color":"dark_red","bold":true}]
execute if score time cmzy_timer matches 160.. run effect give @a blindness 8 0 true
#execute if score time cmzy_timer matches 160 if score normal cmzy_count matches ..0 run scoreboard players set allow_auto_game_end cmzy_count 0
#attribute 
execute if score time cmzy_timer matches 160 as @a[tag=normal] at @s run attribute @s minecraft:generic.movement_speed base set 0.1
execute if score time cmzy_timer matches 160 as @a[tag=killer] at @s run attribute @s minecraft:generic.movement_speed base set 0.15
execute if score time cmzy_timer matches 161 run function cmzy:others/next_level
#fkz
execute if score time cmzy_timer matches 160 as @r[tag=normal,limit=2] at @s run function cmzy:fkz/fkz
#新手教程
execute as @a[scores={trigger=1}] at @s run tag @s add MengXin
execute as @a[scores={trigger=2}] at @s run tag @s add DaLao
execute as @a[scores={trigger=1..}] at @s run scoreboard players reset @s trigger
execute if score time cmzy_timer matches 20..160 run tellraw @a[tag=MengXin] [""]
execute if score time cmzy_timer matches 160 run tellraw @a[tag=MengXin] ["\u00a7e新手提示已开启"]
execute if score time cmzy_timer matches 140..160 run tag @a[tag=DaLao] remove MengXin
execute if score time cmzy_timer matches 150..160 run tellraw @a[tag=DaLao] [""]
