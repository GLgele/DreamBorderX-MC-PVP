execute as @a[tag=killer] run advancement grant @s only cmzy:cmzy/killer
execute as @a[tag=normal] run advancement grant @s only cmzy:cmzy/normal
execute as @a[scores={cmzy_jump=900..}] at @s run advancement grant @s only cmzy:cmzy/jump
execute as @a[scores={cmzy_jump=1200..}] at @s run advancement grant @s only cmzy:cmzy/jumptwo
execute as @a[scores={cmzy_jump=1500..}] at @s run advancement grant @s only cmzy:cmzy/jumpthree
execute as @a[scores={cmzy_sprint=250000..}] at @s run advancement grant @s only cmzy:cmzy/sprint
execute as @a[scores={cmzy_sprint=260000..}] at @s run advancement grant @s only cmzy:cmzy/sprinttwo
execute as @a[scores={cmzy_sprint=270000..}] at @s run advancement grant @s only cmzy:cmzy/sprintthree
execute as @a[scores={cmzy_sprint=280000..}] at @s run advancement grant @s only cmzy:cmzy/sprintfour
execute as @a[scores={cmzy_sprint=290000..}] at @s run advancement grant @s only cmzy:cmzy/sprintfive
execute as @a[scores={cmzy_sneak=10000}] at @s run advancement grant @s only cmzy:cmzy/sneak
execute as @a[scores={cmzy_sneak=12000}] at @s run advancement grant @s only cmzy:cmzy/sneaktwo
execute as @a[scores={cmzy_sneak=14000}] at @s run advancement grant @s only cmzy:cmzy/sneakthree
execute as @a[gamemode=spectator] at @s run scoreboard players reset @s cmzy_sneak

execute if score map cmzy_count matches 1 as @a[gamemode=spectator,x=-30,y=16,z=-39,dx=0,dy=0,dz=0] at @s run advancement grant @s only cmzy:cmzy/map1
execute if score map cmzy_count matches 2 as @a[gamemode=spectator,x=-371,y=32,z=274,dx=0,dy=0,dz=0] at @s run advancement grant @s only cmzy:cmzy/map2
execute if score map cmzy_count matches 3 as @a[gamemode=spectator,x=-270,y=52,z=-67,dx=0,dy=0,dz=0] at @s run advancement grant @s only cmzy:cmzy/map3

execute as @a[scores={cmzy_point=140..}] at @s run advancement grant @s only cmzy:cmzy/experiencefull
