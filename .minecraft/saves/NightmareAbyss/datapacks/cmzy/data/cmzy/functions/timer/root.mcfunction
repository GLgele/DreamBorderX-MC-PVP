#时间线
execute if score game cmzy_count matches 1..6 run scoreboard players add time cmzy_timer 1
#game=0游戏未开始
#game=1游戏开始倒计时
#game=2第一阶段
#game=3第二阶段过场
#game=4第二阶段
#game=5大门开启
#game=6游戏结束
#追击加分
execute as @a[tag=killer_2] at @a[tag=normal,distance=..7,limit=1] run scoreboard players add cumulative_score_all cmzy_timer 1
execute if score cumulative_score_all cmzy_timer matches 80.. at @a[tag=killer] run experience add @a[tag=normal,distance=..7] 1 levels
execute if score cumulative_score_all cmzy_timer matches 80.. at @a[tag=killer_2] run experience add @a[tag=normal,distance=..7] 1 levels
execute if score cumulative_score_all cmzy_timer matches 80.. at @a[tag=killer] run experience add @s 1 levels
execute if score cumulative_score_all cmzy_timer matches 80.. at @a[tag=killer_2] run experience add @s 1 levels
execute if score cumulative_score_all cmzy_timer matches 80.. run scoreboard players reset cumulative_score_all cmzy_timer
#获得积分
execute if score game cmzy_count matches 2..5 run scoreboard players add cumulative_score_killer cmzy_timer 1
execute if score game cmzy_count matches 2..5 run scoreboard players add cumulative_score_normal cmzy_timer 1
#没收非法武器
execute as @a[tag=!fkz] at @s if entity @s[nbt={Inventory:[{tag:{Tags:["fkz","wq"]}}]}] run clear @s carrot_on_a_stick{Tags:["wq"]}
#最高100积分
execute as @a[scores={cmzy_point=101..}] at @s run experience add @s -1 levels
#倒计时
execute if score game cmzy_count matches 1 run function cmzy:timer/timer1
#游戏正式开始
execute if score game cmzy_count matches 2 run function cmzy:timer/timer2
#游戏第二阶段过场
execute if score game cmzy_count matches 3 run function cmzy:timer/timer3
#小门/机关
execute if score game cmzy_count matches 4 if score time cmzy_timer matches ..400 run function cmzy:timer/timer4
#大门开启
execute if score game cmzy_count matches 5 run function cmzy:timer/timer5
#游戏结束
execute if score game cmzy_count matches 6 run function cmzy:timer/timer6
#二阶段道具技能等计时
execute if score game cmzy_count matches 4..5 run function cmzy:timer/propstimer
#擦刀冷却
execute as @a[tag=cadao] if score @s cmzy_timer2 matches 1.. run scoreboard players remove @s cmzy_timer2 1
execute as @a[tag=cadao] if score @s cmzy_timer2 matches ..0 run function cmzy:debug/stop_cooling
execute as @a[tag=cadao,tag=!killer_2] if score @s cmzy_timer2 matches 1.. run replaceitem entity @s hotbar.1 structure_void{display:{Name:'{"text":"\\u00a76\\u00a7l无法使用"}',Lore:['{"text":"\\u00a75武器栏：勿占用此格"}']},Tags:["nowuqi"]} 1
#天赋
function cmzy:talent/root
#擦刀虚弱
execute as @a[tag=!cadao,tag=!feidao] at @s run scoreboard players add @s cmzy_timer2 120
function cmzy:execute/weakness
