execute unless score game cmzy_count matches 2 run bossbar set timer visible false
execute if score time cmzy_timer matches 1 run clear @a
execute if score time cmzy_timer matches 30 run title @a[tag=killer] subtitle ["",{"text":"噩梦开始","color":"red","bold":true}]
execute if score time cmzy_timer matches 30 run title @a title ["",{"text":"梦境降临","color":"blue"}]
execute if score time cmzy_timer matches 200.. run function cmzy:others/phasetwo
#给予玩家积分
execute if score time cmzy_timer matches 40 if score normal cmzy_count matches ..3 run experience add @a[tag=killer] 50 levels
execute if score time cmzy_timer matches 40 if score normal cmzy_count matches ..3 run title @a[tag=killer] actionbar ["",{"text":"A","obfuscated":true,"color":"gray"},{"text":"击杀奖励,获得","color":"gold"},{"text":"20","bold":true,"color":"yellow"},{"text":"点积分","color":"gold"},{"text":"A","obfuscated":true,"color":"gray"}]
execute if score time cmzy_timer matches 40 if score killer_count cmzy_count matches 0 run function cmzy:fkz/fkz_succeed
execute if score time cmzy_timer matches 40 as @a[tag=fkz] at @s run function cmzy:fkz/fkz_fail

#新手提示
execute if score time cmzy_timer matches 60 run tellraw @a[tag=MengXin] ["\u00a7e[提示] ：[\u00a7f\u00a76第一阶段\u00a7e]结束，[\u00a7f\u00a76第二阶段\u00a7e]即将开启。"]
execute if score time cmzy_timer matches 60 run tellraw @a[tag=MengXin] ["\u00a7e[提示] ：[\u00a7f\u00a76第二阶段\u00a7e]将会开放[\u00a7f\u00a7l梦境沙漏\u00a7e]，[\u00a7f\u00a7l零件\u00a7e]，[\u00a7f\u00a7l天赋\u00a7e]，[\u00a7f\u00a7l商店\u00a7e]，[\u00a7f\u00a7l小门交互\u00a7e]，[\u00a7f\u00a7l大门交互\u00a7e]。"]
execute if score time cmzy_timer matches 60 run tellraw @a[tag=MengXin] ["\u00a7e[提示] ：利用好当前[\u00a7f\u00a7l积分\u00a7e]，找到[\u00a7f\u00a7l商店\u00a7e]购买道具！。"]
